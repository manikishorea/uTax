@using EMPPortal.Filters;
@{
    ViewBag.Title = "Last Login Report ";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<link href="~/Content/css/buttons.dataTables.min.css" rel="stylesheet" />
<input type="hidden" id="formid" name="formid" value="active" formid="site947b9d9e-22ce-4584-946a-db1ce6f9c7a1" sitemapid="947b9d9e-22ce-4584-946a-db1ce6f9c7a1" />
<div class="row">
    <div class="col-md-12">
        <div class="panel panel-default">
            <div class="panel-heading">
                <h3 style="padding:10px 0;">Last Login Report</h3>
            </div>
            <div class="table-responsive panel-body">
                <table id="table_LastLoginInfo" class="table table-striped table-hover table-bordered">
                    <thead>
                        <tr>
                            <th width="8%">Master ID</th>
                            <th width="7%">EFIN</th>
                            <th width="10%">User ID</th>
                            <th width="12%">Parent User ID</th>
                            <th width="13%">Company Name</th>
                            <th width="10%">Last Login</th>
                            <th width="20%">DateTime</th>
                            <th width="20%">IP Address</th>
                        </tr>
                    </thead>
                </table>
            </div>
        </div>
    </div>
</div>
<script src="~/Scripts/app/Rpt_FeeSetupReport.js"></script>
<script>
    var today = new Date();
    var Current_Date = today.getDate() + '/' + today.getMonth() + '/' + today.getFullYear() + '  ' + today.getHours() + ':' + today.getMinutes();
    $(document).ready(function () {
        $('#table_LastLoginInfo').DataTable({
            "bSort": false,
            dom: 'Bfrtip',
            buttons: [
                //{
                //    extend: "excel",
                //    text: '<i class="fa fa-file-excel-o"></i> Excel',
                //    classname: 'buttons-html5'
                //},
                {
                    extend: "print",
                    text: '<i class="fa fa-print"></i> Print'
                }, {
                    extend: "csv",
                    text: '<i class="fa fa-file-excel-o"></i> CSV'
                }, {
                    extend: "pdf",
                    text: '<i class="fa fa-file-pdf-o"></i> PDF',
                    orientation: 'landscape',
                    title: 'Last Login Report',
                    message: 'Date Time: ' + Current_Date
                }
            ]
        });
        Fn_LastLoginReport($('#UserId').val());
    });
</script>
