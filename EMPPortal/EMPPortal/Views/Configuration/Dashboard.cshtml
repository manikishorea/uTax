@using EMPPortal.Filters;
@{
    ViewBag.Title = "Dashboard";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<input type="hidden" id="formid" name="formid" value="active" formid="site98a706d7-031f-4c5d-8cc4-d32cc7658b63" sitemapid="98a706d7-031f-4c5d-8cc4-d32cc7658b63" />
<div class="row" id="configdashboard">
    <div class="col-md-12 main-office">
        <div class="page-head">
            <span class="page-title">Main Office & Software Identification Information</span>
            <a id="a_hold" class="hide hold-btn" onclick="HoldUnhold()">Site is Active</a>
        </div>
        <div class="col-md-6 main-office-blk-left">
            <div class="panel panel-default">
                <div class="panel-body">
                    <h3>Main Office Information</h3>
                    <table class="table table-striped table-bordered table-hover ">
                        <tbody>
                            <tr>
                                <td>Company Name </td>
                                <td colspan="2"><span id="spCompanyName"></span></td>
                            </tr>
                            <tr>
                                <td>Business Owner First Name </td>
                                <td colspan="2"><span id="spBusinessOwnerName"></span></td>
                            </tr>
                            <tr>
                                <td>Business Owner Last Name </td>
                                <td colspan="2"><span id="spBusinessOwnerLastName"></span></td>
                            </tr>
                            <tr>
                                <td>Physical Address </td>
                                <td colspan="2"><span id="spPhysicalAddress"></span></td>
                            </tr>
                            <tr>
                                <td>City, State, Zip </td>
                                <td colspan="2"><span id="spCitystatezip"></span></td>
                            </tr>
                            <tr>
                                <td>Office Phone </td>
                                <td colspan="2"><span id="spOfficePhone"></span></td>
                            </tr>
                            <tr>
                                <td>Primary Email </td>
                                <td colspan="2"><span id="spPrimaryemail"></span></td>
                            </tr>
                            <tr>
                                <td>Support Notification Email </td>
                                <td colspan="2"><span id="spSupportemail"></span></td>
                            </tr>
                        </tbody>
                    </table>
                    @if (Convert.ToInt32(Session["entityid"]) == (int)EMPPortal.Utilities.EMPPortalConstants.Entity.uTax)
                    {
                    <div class="clearfix"></div>
                    <div class="form-group">
                        <label>Is uTax allowing SVB to receive SVB fee deposits directly? <b><span id="uTaxNotCollectingSVBFee"></span></b></label>
                        @*<div class="radio" id="dvFourth">
                                <label>
                                    <input type="radio" name="rbuTaxNotCollectingSVBFee" value="1" id="rbuTaxNotCollectingSVBFeeYes">
                                    Yes
                                </label>
                                <label>
                                    <input type="radio" name="rbuTaxNotCollectingSVBFee" value="0" id="rbuTaxNotCollectingSVBFeeNo">
                                    No
                                </label>
                            </div>*@
                    </div>
                    }
                    <!-- books -->
                    <div class="clearfix"> </div>
                    <div class="col-md-12 text-right">
                        <a href="javascript:void(0)" class="btn btn-default btn-info" onclick="GetCustomerInfo_MainOfficeinEDIT()" id="btn_edit">Edit</a>
                        @*@if (Convert.ToInt32(Session["entityid"].ToString()) != (int)EMPPortal.Utilities.EMPPortalConstants.Entity.uTax) {*@
                        <a class="btn btn-default" onclick="DashboardNext('mainsite')" id="btn_next" style="display:none;">Next</a>
                        @*}*@
                    </div>
                </div>
            </div>
        </div>
        <div class="col-md-6 main-office-blk-right">
            <div class="panel panel-default">
                <div class="panel-body">
                    <h3>Software Identification Information</h3>
                    <table class="table table-striped table-bordered table-hover ">
                        <tbody>
                            <tr>
                                <td>EFIN</td>
                                <td><span id="spEFIN"></span> </td>
                            </tr>
                            <tr>
                                <td>Master Identifier</td>
                                <td><span id="spMasterident"></span></td>
                            </tr>
                            <tr>
                                <td>Master User ID</td>
                                <td><span id="spMasterUserID"></span></td>
                            </tr>
                            <tr>
                                <td>Transmitter Password</td>
                                <td><span id="spTransmissionpwd"></span></td>
                            </tr>
                            <tr>
                                <td>MytaxofficePortal.com <br />(User Name):</td>
                                <td><span id="spofficeportalusername"></span></td>
                            </tr>
                            <tr>
                                <td>MytaxofficePortal.com <br />(Password):</td>
                                <td><span id="spoofficeportalpwd"></span></td>
                            </tr>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>
<div id="popupHold" class="modal fade" role="dialog">
    <div class="modal-dialog">
        <!-- Modal content-->
        <div class="modal-content">
            <div class="modal-body">
                <div class="form-group">
                    <strong id="p_hold"></strong>
                    <p id="p_holdDesc" style="margin-top:8px;"></p>
                    <label id="lbl_doldDesc">Enter description </label>
                    <textarea id="txt_doldDesc" rows="3" class="form-control" maxlength="50"></textarea>
                </div>
                <button type="button" class="btn btn-default btn-confirm" onclick="HoldUnholdPopup()">YES</button>
                <button type="button" class="btn btn-default btn-confirm cancel-btn" data-dismiss="modal">NO</button>
            </div>
        </div>
    </div>
</div>
@section scripts {
    @Html.IncludeVersionedJs("/Scripts/app/MoSvbdetails.js")
    @Html.IncludeVersionedJs("/Scripts/app/dashboard.js")
}
