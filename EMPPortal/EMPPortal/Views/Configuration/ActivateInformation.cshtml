            $('#btnEditSubSiteConfig2').attr('href', '/Configuration/SubsiteConfiguration?Id=' + $('#myid').val() + '&entitydisplayid=' + $('#myentitydisplayid').val() + '&ptype=config&entityid=' + $('#myentityid').val());
            $('#btnEditSubSiteConfig3').attr('href', '/Configuration/SubsiteConfiguration?Id=' + $('#myid').val() + '&entitydisplayid=' + $('#myentitydisplayid').val() + '&ptype=config&entityid=' + $('#myentityid').val());
            $('#btnEditFeeSetupConfig').attr('href', '/Configuration/FeesetupConfiguration?Id=' + $('#myid').val() + '&entitydisplayid=' + $('#myentitydisplayid').val() + '&ptype=config&entityid=' + $('#myentityid').val());
            $('#btnEditSVBTRANFeeRem').attr('href', '/Configuration/ServiceBureauTransmission?Id=' + $('#myid').val() + '&entitydisplayid=' + $('#myentitydisplayid').val() + '&ptype=config&entityid=' + $('#myentityid').val());
            $('#btnEditefile').attr('href', '/PaymentOptions/efile?Id=' + $('#myid').val() + '&entitydisplayid=' + $('#myentitydisplayid').val() + '&ptype=config&entityid=' + $('#myentityid').val());
            $('#btnEditBalancee').attr('href', '/PaymentOptions/OutstandingBalance?Id=' + $('#myid').val() + '&entitydisplayid=' + $('#myentitydisplayid').val() + '&ptype=config&entityid=' + $('#myentityid').val());

        } else {

            $('#btnEditMainOfficeConfig').attr('href', '/Configuration/MainOfficeConfiguration');
            $('#btnEditSubSiteConfig1').attr('href', '/Configuration/SubsiteConfiguration');
            $('#btnEditSubSiteConfig2').attr('href', '/Configuration/SubsiteConfiguration');
            $('#btnEditSubSiteConfig3').attr('href', '/Configuration/SubsiteConfiguration');
            $('#btnEditFeeSetupConfig').attr('href', '/Configuration/FeesetupConfiguration');
            $('#btnEditSVBTRANFeeRem').attr('href', '/Configuration/ServiceBureauTransmission');
            $('#btnEditefile').attr('href', '/PaymentOptions/efile');
            $('#btnEditBalancee').attr('href', '/PaymentOptions/efile');

        }
    }


</script>