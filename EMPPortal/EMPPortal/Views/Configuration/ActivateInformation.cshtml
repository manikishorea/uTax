<script>

    $(document).ready(function () {
        getTooltip();

        //  $('#UlMainActivate #liSVBAndTransFeeAddOn').hide();

        $('input[type=text]').attr('disabled', 'disabled');
        $('input[type=radio]').attr('disabled', 'disabled');
        $('input[type=number]').attr('disabled', 'disabled');
        $('select').attr('disabled', 'disabled');
        $('textarea').attr('disabled', 'disabled');
        $('input[type=checkbox]').attr('disabled', 'disabled');

        var EntityId = $('#entityid').val();
        getMainOfficeInformation();
        if ($('#hdnEFINStatus').val() == '16') {
            $('#dvShareEfinQtn').removeClass('hide');
        }

        if ($('#entityid').val() != $('#myentityid').val()) {
            var UserId = $('#myid').val();
            getSubSiteConfig(UserId);
        } else {
            var UserId = $('#UserId').val();
            getSubSiteConfig(UserId);
        }

        getFeeReimbursementInformation();

        var EntityDisplayId = $('#entitydisplayid').val();
        var entityid = $('#entityid').val();
        if (entityid != $('#Entity_SO').val() || entityid != $('#Entity_SOME').val() || entityid != $('#Entity_SOME_SS').val()) { // && EntityDisplayId != $('#BaseEntity_SOME').val() && EntityDisplayId != $('#BaseEntity_SS').val()
            getServiceBureauTransInformation();
        }
    });

    function fnValidConfigStatus() {
        var canSubmit = true;
        var req = {};

        var error = $('#error');

        error.html('');
        error.hide();
        var success = $('#success');
        success.html('');
        success.hide();

        if (!SVBFeesValid()) {
            canSubmit = false;
        }

        if (!TransFeesValid()) {
            canSubmit = false;
        }

        if (!canSubmit) {
            error.show();
            error.append('<p>One or more error(s) have been identified in the Service Bureau and\or Transmission Fee sections. Please have the same resolved to proceed to activate your account.</p>');
            return false;
        }

        if (canSubmit) {
            $('#bank-confimation').modal('show');
        }
    }


    function fnSaveConfigStatus() {
        var error = $('#error');

        error.html('');
        error.hide();
        var success = $('#success');
        success.html('');
        success.hide();

        $('#bank-confimation').modal('show');
        var req = {};

        var UserId = $.trim($('#UserId').val());
        var CustomerId = $.trim($('#UserId').val());
        if ($('#entityid').val() != $('#myentityid').val()) {
            req.CustomerId = $.trim($('#myid').val());
            CustomerId = $.trim($('#myid').val());
        }

        var istax = true;

        $.blockUI({ message: '<img src="../../content/images/loading-img.gif"/>' });
        setTimeout(function () {

            var uri = '/api/CustomerInformation/GetIsCustomerTaxReturn?CustomerId=' + CustomerId;
            ajaxHelper(uri, 'GET', null, false).done(function (res) {
                istax = res;
            })

            var Uri = '/api/CustomerInformation/CustomerConfig?CustomerId=' + CustomerId + '&UserId=' + UserId + '&SiteMapID=4FC65D1B-675F-4985-8022-9E8BD0ED735F';
            ajaxHelper(Uri, 'POST', null, false).done(function (data) {
                if (data != -1) {
                    var message = '';
                    if (data == 1)
                        message = '<p> Activated this information saved Successfully </p>';
                    else if (data == 2)
                        message = '<p> Activated this information saved Successfully </p><p> New Crosslink and EMP User has been created. </p>';
                    else if (data == 3)
                        message = '<p> Activated this information saved Successfully</p><p>Crosslink and EMP User has not created. It will be availble in New Customer Signup</p>'
                    else if (data == 4) {
                        message = '<p> User account activation processed and saved sucessfully.</p><p>Crosslink UserId has been created. But Password has not created. It will be availble in New Customer Signup</p>'
                        error.show();
                        error.append(message);
                    }
                    if (data != 4) {
                        success.show();
                        success.append(message);
                    }

                    UpdateOfficeManagement(CustomerId);

                    $("html, body").animate({ scrollTop: 0 }, "slow");
                    $.unblockUI();
                    if ($('#entityid').val() != $('#myentityid').val()) {
                        window.location.href = "/Configuration/Dashboard?Id=" + CustomerId + "&entitydisplayid=" + $('#myentitydisplayid').val() + '&ptype=config&entityid=' + $('#myentityid').val();
                    } else {
                        if (istax)
                            window.location.href = "/Enrollment/OfficeInformation";
                        else
                            window.location.href = '/Configuration/Dashboard';
                    }
                }
                else {
                    error.show();
                    error.append('<p>  Record not saved. </p>');
                    $.unblockUI();
                    return false;
                }
            });
        }, 500);
    }


    $(function () {

        $(".openbox").each(function () {
            var isChecked = $(this).is(":checked");
            if (isChecked) {
                $(this).closest('.question-block').siblings(".sub-question").show();
            }
        });

        /* on change function */

        $(".openbox").change(function () {
            var isChecked = $(this).is(":checked");
            if (isChecked) {
                $(this).closest('.question-block').siblings(".sub-question").show();
            }
        });

        $(".closebox").change(function () {
            var isChecked = $(this).is(":checked");
            if (isChecked) {
                $(this).closest('.question-block').siblings(".sub-question").hide();
            }
        });



        $(".yesbox").each(function () {
            var isChecked = $(this).is(":checked");
            if (isChecked) {
                $(this).closest('.question-block').siblings(".sub-question").show();
            }
        });

        /* on change function */

        $(".yesbox").change(function () {
            var isChecked = $(this).is(":checked");
            if (isChecked) {
                $(this).closest('.question-block').siblings(".sub-question").show();
            }
        });

        $(".nobox").change(function () {
            var isChecked = $(this).is(":checked");
            if (isChecked) {
                $(this).closest('.question-block').siblings(".sub-question").hide();
            }
        });

        $('input[type=text]').attr('disabled', 'disabled');
        $('input[type=radio]').attr('disabled', 'disabled');
        $('input[type=number]').attr('disabled', 'disabled');
        $('select').attr('disabled', 'disabled');
        $('textarea').attr('disabled', 'disabled');
        $('input[type=checkbox]').attr('disabled', 'disabled');

        fnEditLinkUrls();
    });


    function fnEditLinkUrls() {

        if ($('#entityid').val() != $('#myentityid').val()) {

            $('#btnEditMainOfficeConfig').attr('href', '/Configuration/MainOfficeConfiguration?Id=' + $('#myid').val() + '&entitydisplayid=' + $('#myentitydisplayid').val() + '&ptype=config&entityid=' + $('#myentityid').val());
